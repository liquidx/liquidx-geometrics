{
  "name": "liquidx-geometrics",
  "version": "1.0.0",
  "main": "server.js",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build": "npx webpack --config webpack.dev.js",
    "build-prod": "npx webpack --config webpack.prod.js",
    "watch": "npx webpack --watch  --config webpack.dev.js",
    "start": "npx webpack --config webpack.dev.js && node server.js",
    "start-webpack": "webpack-dev-server --config webpack.dev.js --open",
    "deploy-gcloud": "gcloud app deploy --promote -q --project liquidx-geometrics",
    "deploy-firebase": "firebase deploy",
    "deploy-now": "now",
    "logs-gcloud": "gcloud app logs tail -s default --project liquidx-geometrics",
    "deploy-gcs": "gsutil -m cp -r data-images gs://liquidx-geometrics.a.appspot.com/"
  },
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@google-cloud/storage": "^3.2.1",
    "dat.gui": "^0.7.6",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "firebase": "^6.6.0",
    "handlebars": "^4.5.3",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "p5": "^0.10.2"
  },
  "devDependencies": {
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.3",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-merge": "^4.2.2"
  }
}
